	<!-- Icon Templates -->
	<ControlTemplate x:Key="FileIconTemplate" TargetType="ContentControl">
		<ContentControl Template="{StaticResource IconTemplate}"
                        Content="{StaticResource FileIconData}" />
	</ControlTemplate>

	<ControlTemplate x:Key="MetadataIconTemplate" TargetType="ContentControl">
		<ContentControl Template="{StaticResource IconTemplate}"
                        Content="{StaticResource MetadataIconData}" />
