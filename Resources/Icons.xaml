                        Content="{StaticResource DuplicateFileIconData}" />
	</ControlTemplate>

	<ControlTemplate x:Key="ImageIconTemplate" TargetType="ContentControl">
		<ContentControl Template="{StaticResource IconTemplate}"
                        Content="{StaticResource ImageIconData}" />
